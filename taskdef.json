{
	"family": "ecs-demo",
	"containerDefinitions": [
		{
			"name": "website-project",
			"image": "<IMAGE_NAME>",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 8080,
					"protocol": "tcp",
					"containerPort": 8080
				}
			]
		}
	],
	"requiresCompatibilities": [
		"FARGATE"
	],
	"networkMode": "awsvpc",
	"cpu": "256",
	"memory": "512",
	"executionRoleArn": "arn:aws:iam::873186839494:role/ecsTaskExecutionRole"
}